<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Regression Suite" parallel="tests" thread-count="2">
    <listeners>
        <listener class-name="tests.AnnotationTransformer"/>
        <listener class-name="tests.TestListener"/>
    </listeners>
    <test name="Cart">
        <classes>
            <class name="tests.CartTest"/>
        </classes>
    </test>
    <test name="Checkout">
        <classes>
            <class name="tests.CheckoutTest"/>
        </classes>
    </test>
    <test name="Login">
        <classes>
            <class name="tests.LoginTest"/>
        </classes>
    </test>
    <test name="CheckoutInformation">
        <classes>
            <class name="tests.CheckoutInformationTest"/>
        </classes>
    </test>
    <test name="Sorting">
        <classes>
            <class name="tests.SortTest"/>
        </classes>
    </test>
    <test name="Menu">
        <classes>
            <class name="tests.MenuTest"/>
        </classes>
    </test>
</suite>
